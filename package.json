{
  "name": "graph",
  "version": "1.0.0",
  "description": "javascript no circle svg graph library",
  "main": "index.js",
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "dependencies": {
    "eslint": "^5.11.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "rollup": "^1.0.0",
    "rollup-plugin-babel-minify": "^6.2.0"
  },
  "scripts": {
    "package:dev": "cross-env NODE_ENV=dev ./node_modules/rollup/bin/rollup -c -w",
    "package:prod": "cross-env NODE_ENV=prod ./node_modules/rollup/bin/rollup -c",
    "dev": "npm run lint && npm run package:dev",
    "build": "npm run lint && npm run package:prod",
    "lint": "./node_modules/.bin/eslint ./src/**/*.js",
    "test": "npm run test",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": [
      "./node_modules/.bin/eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jabbla/graph.git"
  },
  "keywords": [
    "javascript",
    "svg",
    "graph"
  ],
  "author": "gloria",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jabbla/graph/issues"
  },
  "homepage": "https://github.com/jabbla/graph#readme"
}
